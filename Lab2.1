class Point
{
    public int X { get; }
    public int Y { get; }

    public Point(int x, int y)
    {
        X = x;
        Y = y;
    }

    // Деконструктор
    public void Deconstruct(out int x, out int y)
    {
        x = X;
        y = Y;
    }
}


class Program
{
    static void Main()
    {
        Point p = new Point(3, 4);
        var (x, y) = p; // деконструкція
        Console.WriteLine($"X: {x}, Y: {y}");
    }
}
